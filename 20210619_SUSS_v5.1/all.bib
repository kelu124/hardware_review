	keywords = {signal processing, reconfigurable architectures, hardware-software codesign, system-on-chip, embedded software, chirplet signal decomposition, coherent averaging, frequency 20 kHz to 20 MHz, paradigm shift, reconfigurable hardware, reconfigurable ultrasonic system-on-chip hardware platform, RUSH platform, split spectrum processing, standardised common platform, ultrasonic signal processing systems},
	pages = {301--308}
}

