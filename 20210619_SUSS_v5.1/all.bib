@article{ranachowski_mechanical_2020,
	title = {Mechanical and {Non}-{Destructive} {Testing} of {Plasterboards} {Subjected} to a {Hydration} {Process}},
	volume = {13},
	copyright = {http://creativecommons.org/licenses/by/3.0/},
	url = {https://www.mdpi.com/1996-1944/13/10/2405},
	doi = {10.3390/ma13102405},
	abstract = {The aim of this study was to investigate the effect of plasterboards\&rsquo; humidity absorption on their performance. Specimens\&rsquo; hydration procedure consisted of consecutive immersing in water and subsequent drying at room temperature. Such a procedure was performed to increase the content of moisture within the material volume. The microstructural observations of five different plasterboard types were performed through optical and scanning electron microscopy. The deterioration of their properties was evaluated by using a three-point bending test and a subsequent ultrasonic (ultrasound testing (UT)) longitudinal wave velocity measurement. Depending on the material porosity, a loss of UT wave velocity from 6\% to 35\% and a considerable decrease in material strength from 70\% to 80\% were observed. Four types of approximated formulae were proposed to describe the dependence of UT wave velocity on board moisture content. It was found that the proposed UT method could be successfully used for the on-site monitoring of plasterboards\&rsquo; hydration processes.},
	language = {en},
	number = {10},
	urldate = {2020-12-13},
	journal = {Materials},
	author = {Ranachowski, Zbigniew and Ranachowski, Przemysław and Dębowski, Tomasz and Brodecki, Adam and Kopec, Mateusz and Roskosz, Maciej and Fryczowski, Krzysztof and Szymków, Mateusz and Krawczyk, Ewa and Schabowicz, Krzysztof},
	month = jan,
	year = {2020},
	keywords = {ultrasound measurements, hydration processes, mechanical properties, moisture content, plasterboards},
	pages = {2405}
}

