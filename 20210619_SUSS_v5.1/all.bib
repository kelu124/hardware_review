@article{wang_research_2021,
	title = {Research on {Golay}-coded excitation in real-time imaging of high frequency ultrasound biomicroscopy},
	volume = {11},
	copyright = {2021 The Author(s)},
	issn = {2045-2322},
	url = {https://www.nature.com/articles/s41598-020-80406-x},
	doi = {10.1038/s41598-020-80406-x},
	abstract = {High frequency ultrasonic imaging provides clinicians with high-resolution diagnostic images and more accurate measurement results. The technique is now widely used in ophthalmology, dermatology, and small animal imaging. However, since ultrasonic attenuation in tissue increases rapidly with increasing frequency, the depth of detection of high frequency ultrasound in tissue is limited to a few millimeters. In this paper, a novel method of using Golay-coded excitation as a replacement for conventional single-pulse excitation in high frequency ultrasound biomicroscopy was proposed, and real-time imaging was realized. While maintaining the transmission voltage and image resolution unchanged, the detection depth can be effectively improved. The ultrasonic transmission frequency is 30 MHz and the transmission voltage is ± 60 V p-p. In this study, 4-bit, 8-bit, and 16-bit coding sequences and decoding compression were used. To verify the effectiveness of the coding sequence in real-time imaging of ultrasound biomicroscopy, we designed a 10-μm diameter line target echo experiment, an ultrasound phantom experiment, and an in vitro porcine eye experiment. The experimental results show that the code/decode method of signal processing can not only maintain a resolution consistent with that of single-pulse transmission, but can also improve the detection depth and signal-to-noise ratio.},
	language = {en},
	number = {1},
	urldate = {2021-01-27},
	journal = {Scientific Reports},
	author = {Wang, Xiaochun and Yang, Jun and Ji, Jianjun and Zhang, Yusheng and Zhou, Sheng},
	month = jan,
	year = {2021},
	pages = {1848}
}

@inproceedings{wiercigroch_ultrasound-guided_2021,
	title = {Ultrasound-guided needle placement system optimized for translation to {Mauritania}},
	volume = {11598},
	url = {https://www.spiedigitallibrary.org/conference-proceedings-of-spie/11598/115980X/Ultrasound-guided-needle-placement-system-optimized-for-translation-to-Mauritania/10.1117/12.2582012.short},
	doi = {10.1117/12.2582012},
	abstract = {{\textless}strong{\textgreater}PURPOSE:{\textless}/strong{\textgreater} Point-of-care ultrasound image-guided therapies (POCUS IGT) transcend geographic and socioeconomic boundaries and help bringing modern therapies to underserved communities and countries. Unfortunately, current commercial systems are not feasible to deploy in Mauritania due to prohibitive costs of purchase, support, and operation. We present the development of a versatile POCUS IGT system, optimized for financial and operating conditions in Mauritania. We aimed to create a system that is functionally similar to the popular CIVCO product, but costs only a small fraction of the price due to the support of inexpensive ultrasound scanners and its use of open-source software. {\textless}strong{\textgreater}METHODS:{\textless}/strong{\textgreater} A 3D-printed plastic needle guide with multiple guide channels was designed to securely fit around the ultrasound probe, placed in a sterile cover, and fitted with a sterile guide sleeve. Open source targeting software was developed to overlay selectable needle trajectories on the live ultrasound image. {\textless}strong{\textgreater}RESULTS:{\textless}/strong{\textgreater} The assembly and workflow were found to be similar to CIVCO’s. The operator was able to accurately reach targets in the entire workspace. Excluding the price of the ultrasound scanner, the use of system costs around \$1.00 per patient. {\textless}strong{\textgreater}CONCLUSION:{\textless}/strong{\textgreater} A POCUS IGT system was developed using free open-source guidance software that supports low-cost ultrasound scanners, offering similar performance to the leading commercial system for 5\% of price.},
	urldate = {2021-03-18},
	booktitle = {Medical {Imaging} 2021: {Image}-{Guided} {Procedures}, {Robotic} {Interventions}, and {Modeling}},
	publisher = {International Society for Optics and Photonics},
	author = {Wiercigroch, Julia and Ungi, Tamas and Idriss, Ahmedou Moulaye and Tfeil, Yahya and Kikinis, Ron and Mousavi, Parvin and Fichtinger, Gabor},
	month = feb,
	year = {2021},
	pages = {115980X}
}

