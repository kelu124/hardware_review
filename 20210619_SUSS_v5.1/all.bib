	pages = {531--544},
	file = {IEEE Xplore Abstract Record:/home/kelu/Zotero/storage/2H4N7K9K/Dalitz et al. - 2015 - Point spread functions and deconvolution of ultras.html:text/html;IEEE Xplore Full Text PDF:/home/kelu/Zotero/storage/IBNWZJ8A/Dalitz et al. - 2015 - Point spread functions and deconvolution of ultras.pdf:application/pdf}
