	keywords = {Ultrasonic measurements, Thin liquid film thickness, Two-phase flow},
	pages = {67--78},
	file = {ScienceDirect Snapshot:/home/kelu/Zotero/storage/QBVFEE7M/Al-Aufi et al. - 2019 - Thin film thickness measurements in two phase annu.html:text/html}
}

