	author = {Yang, X. and Chen, Z. and Hettiarachchi, N. and Yan, J. and Liu, H.},
	year = {2019},
	keywords = {Ultrasonic imaging, Transducers, Muscles, Prosthetics, Biomedical monitoring, Sensors, Monitoring, Muscle contraction monitoring, prosthesis control, wearable ultrasound sensing.},
	pages = {1--10}
}

@article{yang_simultaneous_2020,
	title = {Simultaneous {Prediction} of {Wrist}/{Hand} {Motion} via {Wearable} {Ultrasound} {Sensing}},
	volume = {28},
	issn = {1558-0210},
	doi = {10.1109/TNSRE.2020.2977908},
	abstract = {The ability to predict wrist and hand motions simultaneously is essential for natural controls of hand protheses. In this paper, we propose a novel method that includes subclass discriminant analysis (SDA) and principal component analysis for the simultaneous prediction of wrist rotation (pronation/supination) and finger gestures using wearable ultrasound. We tested the method on eight finger gestures with concurrent wrist rotations. Results showed that SDA was able to achieve accurate classification of both finger gestures and wrist rotations under dynamic wrist rotations. When grouping the wrist rotations into three subclasses, about 99.2 ± 1.2\% of finger gestures and 92.8 ± 1.4\% of wrist rotations can be accurately classified. Moreover, we found that the first principal component (PC1) of the selected ultrasound features was linear to the wrist rotation angle regardless of finger gestures. We further used PC1 in an online tracking task for continuous wrist control and demonstrated that a wrist tracking precision (R2) of 0.954 ± 0.012 and a finger gesture classification accuracy of 96.5 ± 1.7\% can be simultaneously achieved, with only two minutes of user training. Our proposed simultaneous wrist/hand control scheme is training-efficient and robust, paving the way for musculature-driven-artificial hand control and rehabilitation treatment.},
	number = {4},
	journal = {IEEE Transactions on Neural Systems and Rehabilitation Engineering},
	author = {Yang, X. and Yan, J. and Fang, Y. and Zhou, D. and Liu, H.},
	month = apr,
	year = {2020},
	keywords = {biomedical ultrasonics, Ultrasonic imaging, biomechanics, Muscles, Training, gesture recognition, Principal component analysis, artificial limbs, concurrent wrist rotations, continuous wrist control, dynamic wrist rotations, finger gesture classification, hand motions, hand protheses, medical control systems, motion control, musculature-driven-artificial hand control, patient rehabilitation, principal component, principal component analysis, simultaneous wrist/hand control, subclass discriminant analysis, Subclass discriminant analysis, Target tracking, Task analysis, wearable ultrasound sensing, Wrist, wrist rotation angle},
	pages = {970--977}
}

