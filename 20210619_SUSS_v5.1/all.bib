	month = apr,
	year = {2017},
	pages = {V001T08A008},
	file = {Full Text PDF:/home/kelu/Zotero/storage/4TIFJP3J/Zhu et al. - 2017 - Programmable Ultrasound Phased Array Therapy Syste.pdf:application/pdf}
