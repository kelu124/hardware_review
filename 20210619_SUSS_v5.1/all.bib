	keywords = {Abdomen, Adipose tissue, Analysis of variance, Fats, Research validity, Sports, Technicians, Triceps},
	pages = {e0153146},
	file = {Full Text PDF:/home/kelu/Zotero/storage/8U4Z3VS9/Wagner et al. - 2016 - Validity and Reliability of A-Mode Ultrasound for .pdf:application/pdf;Snapshot:/home/kelu/Zotero/storage/Q3TSYDE8/article.html:text/html}
