	month = feb,
	year = {2006},
	keywords = {Ultrasonic variables measurement, Digital signal processing, Analytical models, acoustic signal processing, Computational modeling, Computer-aided performance assessment, demodulation, Demodulation, digital signal-processing method, distance measurement, Distortion measurement, estimation theory, High performance computing, quadrature demodulation, Shape measurement, Signal to noise ratio, statistical experimental design, Testing, time-of-flight (TOF) estimation, TOF-based distance measurements, ultrasonic measurement, ultrasonic signals, ultrasonic time-of-flight estimation, ultrasonic-based measurement},
