	keywords = {biomedical ultrasonics, Ultrasonic imaging, high voltage pulser, biomedical electronics, medical imaging, Prototypes, Computer architecture, ultrasound pulses, Chirp, medical ultrasound imaging, 9-level pulse-shaping technique, chip-level simulations, Chirp modulation, flexible hardware architecture, four-channel high-voltage transmitter chip, Frequency modulation, integrated circuits, linear frequency modulated chirp pulse signal, Low voltage, multilevel ultrasound drivers},
	pages = {1--4}
}

@article{hidayat_determination_2020,
	title = {Determination of generated ultrasonic wave characteristics by a bipolar square burst excitation},
	volume = {1568},
	issn = {1742-6596},
	url = {https://doi.org/10.1088%2F1742-6596%2F1568%2F1%2F012007},
	doi = {10.1088/1742-6596/1568/1/012007},
	abstract = {Transducer excitation signals is an important element in the control of generated ultrasonic wave characteristics, particularly in a high frequency ultrasonic measurement system in which power and sensitivity are crucial aspects. This work investigates the application of unipolar and bipolar square burst pulses as the transducer excitation signals on the generated ultrasonic wave characteristics. Transducers with various center frequencies (0.2 – 10 MHz) are excited by unipolar and bipolar square burst pulses, respectively, at a particular number of pulse cycle (N-cycle). The investigation results showed that the generated/received ultrasonic wave powers excited by the bipolar square burst pulses are higher than that of unipolar square pulses. It is indicated with the increase of received signals amplitude. In conclusion, the application of bipolar square burst pulses improved the performances of the generated ultrasonic waves by increasing its power.},
