@article{shahshahani_ultrasound_2018,
	title = {Ultrasound {Sensors} for {Diaphragm} {Motion} {Tracking}: {An} {Application} in {Non}-{Invasive} {Respiratory} {Monitoring}},
	volume = {18},
	copyright = {http://creativecommons.org/licenses/by/3.0/},
