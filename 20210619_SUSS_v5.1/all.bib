	keywords = {open design, 3D printing, open hardware, custom designs, customization, distributed manufacturing, FOSH, free and open-source hardware, free and open-source software, open science hardware, open scientific hardware, OpenSCAD, OScH, P2P, P2P manufacturing, RepRap, scientific equipment, slide dryer},
	pages = {2}
}

@article{silva_embedded_2020,
