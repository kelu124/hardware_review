	keywords = {Capsule endoscopy, High-frequency ultrasound, Micro-ultrasound capsule endoscopy, porcine animal study},
	pages = {796--804},
	file = {ScienceDirect Full Text PDF:/home/kelu/Zotero/storage/UT43YPQX/Qiu et al. - 2020 - Ultrasound Capsule Endoscopy With a Mechanically S.pdf:application/pdf;ScienceDirect Full Text PDF:/home/kelu/Zotero/storage/URZHNPSX/Qiu et al. - 2020 - Ultrasound Capsule Endoscopy With a Mechanically S:application/octet-stream;ScienceDirect Snapshot:/home/kelu/Zotero/storage/GUQ6FTSH/S0301562919316230.html:text/html}
