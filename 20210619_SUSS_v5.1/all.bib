	author = {Boujut, Jean-François and Pourroy, Franck and Marin, Philippe and Dai, Jason and Richardot, Gilles},
	month = jul,
	year = {2019},
	keywords = {Collaborative design, Design process, Open innovation, Open source design, Organisation of product development},
